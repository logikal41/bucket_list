<h1>Saved Bucket Lists</h1>

<%= link_to 'Create New Bucket List', new_list_path %>

<hr />

<ul>
<% @lists.each do |list| %>
    <li><h2><%= list.name %></h2>
    <%= link_to 'Show', list %>
    <%= link_to 'Edit', edit_list_path(list) %>
    <%= link_to 'Delete', list, method: :delete %>
    </li>
<% end %>
</ul>